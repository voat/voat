



<div class="markdownEditor">
    <!-- Markdown Editor Table Creator Sub Menu -->
    <div class="markdownEditorSubMenu" style="display: none;">
        <input type="hidden" name="cols" id="numCols" value="1">
        <input type="hidden" name="rows" id="numRows" value="1">
        <a class="markdownEditorImgButton" style="background-position: 0px -32px;" alt="more_column" title="More Columns" onclick="incColumns($(this.parentElement).find('#numCols')[0],$(this.parentElement).find('#numColsDisplay')[0]);"></a>
        <a class="markdownEditorImgButton" style="background-position: -32px -32px;" alt="less_column" title="Less Columns" onclick="decColumns($(this.parentElement).find('#numCols')[0],$(this.parentElement).find('#numColsDisplay')[0]);"></a>
        <a class="markdownEditorImgButton" style="background-position: -64px -32px;" alt="more_rows" title="More Rows" onclick="incRows($(this.parentElement).find('#numRows')[0],$(this.parentElement).find('#numRowsDisplay')[0]);"></a>
